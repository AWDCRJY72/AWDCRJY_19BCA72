import java.io.*;
import java.util.*;
class harshad
{
public static void main(String ar[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int rem,sum=0;
while(n>0)
{
rem=n%10;
sum+=rem;
n/=10;
}
if(n%sum==0)
  System.out.println("harshad number");
else
  System.out.println("not harshad number");
}
}

output:

C:\java>javac harshad.java

C:\java>java harshad
156
harshad number
